{
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "build": "npm run generate:scene && vite build",
    "deploy": "npm run predeploy && ghpages git@github.com:supermedium/moonrider.git -p .ghpages && rm -rf .ghpages",
    "generate:scene": "node scripts/generate-scene-html.mjs",
    "lint": "semistandard -v | snazzy",
    "lint:fix": "semistandard --fix",
    "predeploy": "npm run build && shx mkdir -p .ghpages && cp -r build/* assets vendor CNAME .ghpages",
    "start": "npm run generate:scene && vite --host 0.0.0.0 --port 3000"
  },
  "dependencies": {
    "aframe": "^1.7.1",
    "aframe-aabb-collider-component": "^3.2.2",
    "aframe-atlas-uvs-component": "^3.0.0",
    "aframe-audioanalyser-component": "^6.1.0",
    "aframe-event-decorators": "^1.0.2",
    "aframe-event-set-component": "^4.2.1",
    "aframe-geometry-merger-component": "^2.0.1",
    "aframe-haptics-component": "^1.6.3",
    "aframe-log-component": "^1.0.7",
    "aframe-orbit-controls": "^1.3.2",
    "aframe-proxy-event-component": "^2.1.0",
    "aframe-slice9-component": "^1.0.0",
    "aframe-state-component": "6.7.0",
    "aframe-thumb-controls-component": "^2.0.3",
    "ansi-html": "0.0.7",
    "debug": "^4.4.3",
    "fflate": "^0.8.2",
    "firebase": "^7.24.0",
    "ip": "1.1.5",
    "lodash.debounce": "^4.0.8",
    "profane-words": "^1.6.0",
    "promise-polyfill": "^8.3.0"
  },
  "devDependencies": {
    "cross-env": "4.0.0",
    "ghpages": "0.0.10",
    "nunjucks": "^3.2.4",
    "semistandard": "10.0.0",
    "shx": "^0.2.2",
    "snazzy": "7.0.0",
    "superagent": "^10.2.2",
    "vite": "^7.3.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "build/**"
    ]
  },
  "engines": {
    "node": ">=24.10.0",
    "npm": ">=11.6.0"
  }
}
